{
  "name": "Photo Zoom for Twitter",
  "version": "1.3.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://twitter.com/*","https://twitter.com/*"],
      "js": ["jquery.min.js", "contentscript.js"],
      "all_frames":true
    }
  ],
  "description": "Adds large size preview images to your Twitter web.",
  "web_accessible_resources": [
        "loading.gif"
    ],
  "icons": {"128": "logo_128.png" }
}